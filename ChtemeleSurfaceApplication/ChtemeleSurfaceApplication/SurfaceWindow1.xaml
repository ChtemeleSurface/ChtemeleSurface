            <s:TagVisualizationDefinition Value="0x79" TagRemovedBehavior="Fade" Source="Tag.xaml"/>
            <s:TagVisualizationDefinition Value="0x7A" TagRemovedBehavior="Fade" Source="Tag.xaml"/>
            
